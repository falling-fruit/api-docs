[  {    "type": "get",    "url": "/clusters.json",    "title": "Get clusters",    "version": "0.2.0",    "name": "GetClusters",    "group": "Clusters",    "description": "<p>Get location clusters.</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Integer[]",            "optional": true,            "field": "types",            "description": "<p>IDs of types to count (or all if empty or missing).</p>"          },          {            "group": "Parameter",            "type": "Integer",            "optional": true,            "field": "zoom",            "defaultValue": "0",            "description": "<p>Zoom level, where world is divided into a 2^zoom x 2^zoom grid.</p>"          },          {            "group": "Parameter",            "type": "Boolean",            "optional": true,            "field": "muni",            "defaultValue": "true",            "description": "<p>Whether to count locations from municipal tree inventories.</p>"          },          {            "group": "Parameter",            "type": "Number[]",            "optional": true,            "field": "bounds",            "defaultValue": "-180,180,-90,90",            "description": "<p>Map bounding box [W Longitude, E Longitude, S Latitude, N Latitude] in WGS84 decimal degrees.</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "key",            "description": "<p>API key.</p>"          }        ]      }    },    "examples": [      {        "title": "Example usage:",        "content": "/clusters.json?key=APIKEY&zoom=0",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Object[]",            "optional": false,            "field": "clusters",            "description": "<p>List of clusters.</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "cluster.lng",            "description": "<p>Longitude in WGS84 decimal degrees, as the mean of the location longitudes.</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "cluster.lat",            "description": "<p>Latitude in WGS84 decimal degrees, as the mean of the location latitudes.</p>"          },          {            "group": "Success 200",            "type": "Integer",            "optional": false,            "field": "cluster.count",            "description": "<p>Total number of locations.</p>"          }        ]      }    },    "filename": "./apidoc.js",    "groupTitle": "Cluster endpoints",    "groupDescription": "<p>Clusters summarize the number of locations available in an area.</p>"  }]
